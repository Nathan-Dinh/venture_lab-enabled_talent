{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug enable_talent_rest_api",
      "program": "${workspaceFolder}/services/enable_talent_rest_api/dist/app.js",
      "preLaunchTask": "npm: build - enable_talent_rest_api",
      "cwd": "${workspaceFolder}/services/enable_talent_rest_api",
      "env": {
        "NODE_ENV": "development"
      },
      "envFile": "${workspaceFolder}/services/enable_talent_rest_api/.env",
      "console": "integratedTerminal",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/services/enable_talent_rest_api/dist/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug enable_talent_rest_api (with tsx)",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "tsx",
        "watch",
        "--inspect"
      ],
      "args": [
        "${workspaceFolder}/services/enable_talent_rest_api/src/app.ts"
      ],
      "cwd": "${workspaceFolder}/services/enable_talent_rest_api",
      "env": {
        "NODE_ENV": "development"
      },
      "envFile": "${workspaceFolder}/services/enable_talent_rest_api/.env",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to enable_talent_rest_api",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    }
  ]
}
